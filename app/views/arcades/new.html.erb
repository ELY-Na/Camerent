<div class="container">
<%= simple_form_for [@arcade] do |f| %>
  <%= f.text_field :name, placeholder: 'Name' %>
  <%= f.text_field :address, placeholder: 'Adress' %>
  <%= f.text_field :price, placeholder: 'Price' %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>
</div>
