<%= simple_form_for [ @arcade] do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :address %>
  <%= f.text_field :zip_code %>
  <%= f.text_field :city %>
  <%= f.file_field :photo,title: "Please choose file"%><%= cl_image_tag @arcade.photo%>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>
